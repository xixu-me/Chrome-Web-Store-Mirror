{
  "env": {
    "es2022": true,
    "worker": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "addEventListener": "readonly",
    "fetch": "readonly",
    "Headers": "readonly",
    "Request": "readonly",
    "Response": "readonly",
    "URL": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-var": "error",
    "prefer-const": "error"
  }
}